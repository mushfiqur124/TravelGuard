{
  "name": "travelguard-extension",
  "version": "1.0.0",
  "description": "Chrome extension that provides instant travel vaccination recommendations for healthcare providers",
  "scripts": {
    "scrape": "node scripts/scraper.js",
    "scrape:test": "node scripts/production-test.js",
    "build": "node scripts/build.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js",
    "prepare-data": "npm run scrape && npm run build",
    "production-ready": "npm run scrape:test && echo 'Production test passed! Run npm run scrape for full data collection'"
  },
  "keywords": [
    "chrome-extension",
    "healthcare",
    "vaccination",
    "travel-health",
    "pharmacy",
    "medme"
  ],
  "author": "MedMe Health",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "jsdom": "^23.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "private"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "collectCoverageFrom": [
      "content-scripts/**/*.js",
      "background/**/*.js",
      "popup/**/*.js",
      "scripts/**/*.js",
      "!**/node_modules/**",
      "!**/tests/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "webextensions": true,
      "es2022": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  }
}
